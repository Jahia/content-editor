<j = 'http://www.jahia.org/jahia/1.0'>
<jmix = 'http://www.jahia.org/jahia/mix/1.0'>
<jnt = 'http://www.jahia.org/jahia/nt/1.0'>
<mix = 'http://www.jcp.org/jcr/mix/1.0'>
<nt = 'http://www.jcp.org/jcr/nt/1.0'>

[jmix:mix1] mixin
    - propMix1 (string)
[jmix:mix2] mixin
    - propMix2 (string)


[jnt:simple] > nt:base
    - prop (string)

[jnt:simpleWithExtends] > nt:base
    - prop (string)

[jnt:simpleRank] > nt:base
    - prop1 (string)
    - prop2 (string)
    - prop3 (string)

[jnt:simpleWithMix] > nt:base, jmix:mix1, jmix:mix2
    - prop (string)

[jnt:unstructuredNews] > jnt:content, jmix:unstructured, jmix:editorialContent

[jmix:extendsSimpleMixin] mixin
extends=jnt:simpleWithExtends
    - extension (string)

[jnt:defaultOverrideContent] > jnt:content, jmix:cache, jmix:categorized, jmix:keywords, jmix:tagged
