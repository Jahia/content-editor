{
    "name": "@jahia/content-editor",
    "version": "3.0.0",
    "scripts": {
        "build": "yarn lint && yarn test && yarn webpack",
        "webpack": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js",
        "build:analyze": "yarn build --analyze",
        "build:production": "yarn build -p",
        "build:production-analyze": "yarn build:production --analyze",
        "watch": "yarn webpack --watch",
        "clean": "rimraf *.log src/main/resources/javascript/apps",
        "clean:all": "yarn clean && rimraf node_modules node",
        "test": "jest --coverage",
        "tdd": "jest --watch",
        "lint": "eslint --ext js,jsx,json . && stylelint './src/**/*.scss'",
        "lint:fix": "eslint --ext js,jsx,json --fix . && stylelint --fix './src/**/*.scss'",
        "storybook": "STORYBOOK_ENV=true start-storybook -p 6006",
        "build-storybook": "STORYBOOK_ENV=true build-storybook",
        "publish-storybook": "rsync -r ./storybook-static/ jahia@design.int.jahia.com:/var/www/vhosts/contenteditor.jahia.design/html"
    },
    "description": "Jahia Content Editor",
    "main": "index.js",
    "repository": "git@github.com:Jahia/content-editor.git",
    "author": "Jahia Core Team <core@jahia.com>",
    "license": "SEE LICENSE IN LICENSE.txt FILE",
    "dx-extends": {
        "@jahia/jahia-ui-root": "^0.0.1"
    },
    "jahia": {
        "apps": {
            "jahia": [
                "javascript/apps/content-editor-ext.bundle${buildTimeStamp}.js",
                "/cms/render/default/en/settings.contentEditor.js"
            ]
        }
    },
    "dependencies": {
        "@apollo/react-hooks": "^3.1.4",
        "@jahia/data-helper": "^0.0.19",
        "@jahia/design-system-kit": "^1.1.5",
        "@jahia/icons": "^1.1.1",
        "@jahia/moonstone": "^0.16.0",
        "@jahia/react-material": "^2.1.2",
        "@jahia/ui-extender": "^0.4.13",
        "@material-ui/core": "^3.9.3",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/lab": "^3.0.0-alpha.30",
        "ckeditor4-react": "^1.1.0",
        "connected-react-router": "^6.8.0",
        "dayjs": "1.8.21",
        "formik": "1.5.8",
        "graphql-tag": "^2.10.3",
        "i18next": "^19.4.0",
        "prop-types": "^15.6.2",
        "react": "^16.13.1",
        "react-apollo": "^3.1.4",
        "react-day-picker": "^7.4.0",
        "react-dnd": "^10.0.2",
        "react-dnd-html5-backend": "^10.0.2",
        "react-dom": "^16.13.1",
        "react-dropdown-tree-select": "^2.3.2",
        "react-file-viewer": "^1.1.0",
        "react-i18next": "^11.3.4",
        "react-infinite-scroller": "^1.2.4",
        "react-pdf-js": "^5.1.0",
        "react-redux": "^7.2.0",
        "react-router-dom": "^5.1.2",
