{
    "name": "@jahia/content-editor",
    "version": "1.0.0",
    "scripts": {
        "build": "yarn lint && yarn test && yarn webpack",
        "webpack": "node --max_old_space_size=2048 ./node_modules/webpack/bin/webpack.js",
        "build:analyze": "yarn build --analyze",
        "build:production": "yarn build -p",
        "build:production-analyze": "yarn build:production --analyze",
        "watch": "yarn webpack --watch",
        "clean": "rimraf *.log src/main/resources/javascript/apps",
        "clean:all": "yarn clean && rimraf node_modules node",
        "test": "jest --coverage",
        "tdd": "jest --watch",
        "lint": "eslint --ext js,jsx,json .",
        "lint:fix": "eslint --ext js,jsx,json --fix .",
        "storybook": "STORYBOOK_ENV=true start-storybook -p 6006",
        "build-storybook": "STORYBOOK_ENV=true build-storybook",
        "publish-storybook": "rsync -r ./storybook-static/ jahia@design.int.jahia.com:/var/www/vhosts/contenteditor.jahia.design/html"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn test"
        }
    },
    "description": "Content and Media Manager for DX",
    "main": "index.js",
    "repository": "git@github.com:Jahia/content-editor.git",
    "author": "Jahia Core Team <core@jahia.com>",
    "license": "SEE LICENSE IN LICENSE.txt FILE",
    "dx-extends": {
        "@jahia/content-manager": "^1.3.0"
    },
    "dependencies": {
        "@jahia/apollo-dx": "^1.1.0",
        "@jahia/design-system-kit": "^1.1.2",
        "@jahia/icons": "^1.1.0",
        "@jahia/react-apollo": "^1.1.0",
        "@jahia/react-material": "^1.1.0",
        "@jahia/registry": "^1.0.0-beta.20190724063719",
        "@material-ui/core": "^3.9.3",
        "@material-ui/icons": "^3.0.1",
        "@material-ui/lab": "^3.0.0-alpha.30",
        "ckeditor4-react": "^1.0.1",
        "dayjs": "1.8.14",
        "formik": "1.5.8",
        "graphql-tag": "^2.9.1",
        "i18next": "^19.0.3",
        "prop-types": "^15.6.2",
        "react": "^16.8.6",
        "react-apollo": "^2.5.3",
        "react-apollo-hooks": "^0.4.5",
        "react-day-picker": "^7.3.0",
        "react-dom": "^16.8.6",
        "react-dropdown-tree-select": "^2.1.1",
        "react-file-viewer": "^1.1.0",
        "react-i18next": "^11.2.7",
        "react-pdf-js": "^4.0.2",
        "react-redux": "^5.0.7",
        "react-select": "^3.0.4",
        "redux": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.1.5",
        "@babel/core": "^7.4.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/polyfill": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "@babel/preset-react": "^7.0.0",
        "@jahia/eslint-config": "^1.1.0",
        "@jahia/test-framework": "^1.1.1",
        "@storybook/addon-actions": "^5.2.8",
        "@storybook/addon-knobs": "^5.2.8",
        "@storybook/addon-links": "^5.2.8",
        "@storybook/addon-notes": "^5.2.8",
        "@storybook/addons": "^5.2.8",
        "@storybook/react": "^5.2.8",
        "apollo-cache-inmemory": "^1.4.3",
        "babel-jest": "^24.5.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-lodash": "^3.3.4",
        "case-sensitive-paths-webpack-plugin": "^2.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "eslint": "^6.7.2",
        "eslint-loader": "3.0.3",
        "eslint-plugin-json": "^2.0.1",
        "file-loader": "^5.0.2",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "redux-mock-store": "^1.5.4",
        "redux-test-utils": "^0.3.0",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10"
    }
}
